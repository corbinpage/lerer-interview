

<div class="container">

        <div class="row">

            <div class="col-lg-12">
                <h1 class="page-header">We have Relationships with:</h1>
            </div>


            <% @partners.each do |partner| %>
              <div class="col-lg-3 col-md-4 col-xs-6 thumb">
                  <a class="thumbnail" href="<%= partner_path(partner.id) %>">
                      <img class="img-responsive partner-logo" src="/assets/<%= partner.custom2 %>">
                      <!-- <img class="img-responsive" src="http://placehold.it/400x300"> -->
                  </a>
              </div>
            <% end %>

        </div>

        <hr>



<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Owner</th>
      <th>Custom1</th>
      <th>Custom2</th>
      <th>Custom3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partners.each do |partner| %>
      <tr>
        <td><%= partner.name %></td>
        <td><%= partner.description %></td>
        <td><%= partner.owner %></td>
        <td><%= partner.custom1 %></td>
        <td><%= partner.custom2 %></td>
        <td><%= partner.custom3 %></td>
        <td><%= link_to 'Show', partner %></td>
        <td><%= link_to 'Edit', edit_partner_path(partner) %></td>
        <td><%= link_to 'Destroy', partner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partner', new_partner_path %>


    </div>
